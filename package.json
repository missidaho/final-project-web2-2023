{
    "name": "sass-compile",
    "version": "1.0.0",
    "description": "A simple sass compile config.",
    "main": "index.js",
    "scripts": {
        "dev": "sass --watch --update --style=expanded src/scss:assets/css --watch indexpages",
        "build": "sass --no-source-map --style=compressed src/scss:assets/css --watch componentsass"
    },
    "author": "Adam",
    "license": "ISC",
    "devDependencies": {
        "sass": "^1.62.0"
    },
    "dependencies": {
        "node": "^20.1.0"
    }
}
